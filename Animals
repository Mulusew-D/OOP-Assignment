class Animal:
    def move(self):
        pass  # generic method


class Dog(Animal):
    def move(self):
        print("Running ğŸ•")


class Bird(Animal):
    def move(self):
        print("Flying ğŸ¦")


class Fish(Animal):
    def move(self):
        print("Swimming ğŸŸ")


# Test polymorphism
animals = [Dog(), Bird(), Fish()]

for a in animals:
    a.move()
