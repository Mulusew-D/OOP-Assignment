class Animal:
    def move(self):
        pass  # generic method


class Dog(Animal):
    def move(self):
        print("Running 🐕")


class Bird(Animal):
    def move(self):
        print("Flying 🐦")


class Fish(Animal):
    def move(self):
        print("Swimming 🐟")


# Test polymorphism
animals = [Dog(), Bird(), Fish()]

for a in animals:
    a.move()
